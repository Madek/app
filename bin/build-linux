#!/usr/bin/env bash
set -eoux
rm -rf ./madek-app-darwin-x64

./node_modules/electron-packager/cli.js \
  --overwrite --platform=linux --arch=x64 --electron-version=1.6.0 \
  --icon=Madek.icns \
  ./app/prod madek-app

